<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back_norm"
    >




    <ImageButton
        android:id="@+id/btn_backstore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/back_icon"
        android:backgroundTint="@android:color/transparent"
        android:onClick="backtolevelfromend"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#4C0000"
        android:textStyle="bold|italic"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


    <ImageView

        android:id="@+id/imgendscreen"
        android:layout_width="200dp"
        android:layout_height="180dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/imageButton3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="YOU WIN....."
        android:id="@+id/textView3"
        android:textColor="#4C0000"
        android:textSize="30dp"
        android:textStyle="bold|italic"
        android:layout_above="@+id/imgendscreen"
        android:layout_toRightOf="@+id/imageButton3"
        android:layout_toEndOf="@+id/imageButton3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="SCORE  :"
        android:id="@+id/textView4"
        android:textColor="#4C0000"
        android:textSize="25dp"
        android:textStyle="bold|italic"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/textView3"
        android:layout_alignEnd="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="TIME TAKEN :"
        android:textStyle="bold|italic"
        android:textColor="#4C0000"
        android:textSize="25dp"
        android:id="@+id/textView6"
        android:layout_below="@+id/textView4"
        android:layout_alignRight="@+id/textView4"
        android:layout_alignEnd="@+id/textView4"
        android:layout_marginTop="52dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/endlevelscore"
        android:text=""
        android:textStyle="bold|italic"
        android:textColor="#4C0000"
        android:textSize="22dp"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/endleveltime"
        android:layout_alignStart="@+id/endleveltime" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/endleveltime"
        android:text=""
        android:textStyle="bold|italic"
        android:textColor="#4C0000"
        android:textSize="22dp"
        android:layout_alignTop="@+id/textView6"
        android:layout_toRightOf="@+id/textView6"
        android:layout_toEndOf="@+id/textView6"
        android:layout_marginLeft="45dp"
        android:layout_marginStart="45dp" />

    <ImageButton
        android:layout_width="180dp"
        android:layout_height="80dp"
        android:id="@+id/imageButton3"
        android:layout_margin="20dp"
        android:background="@drawable/logo_back"
        />
</RelativeLayout>